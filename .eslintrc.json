{
  "extends": ["react-app", "react-app/jest", "plugin:import/recommended", "plugin:import/typescript"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "warn",
    "import/order": ["warn", { "newlines-between": "always" }]
  },
  "settings": {
    "import/resolver": {
      "node": {},
      "typescript": {}
    }
  }
}
